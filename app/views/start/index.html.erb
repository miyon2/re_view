<h1>this is start page</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alls.each do |all| %>
      <tr>
        <td><%= all.title %></td>
        <td><%= all.content %></td>
        <td><%= link_to 'Show', all %></td>
        <td><%= link_to 'Destroy', all, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
